<h1>Profile Page</h1>
<div>
  <%= image_tag @city.image, size: "200x200" %>
  <h1><%= @city.name %> </h1>
</div>
<h3>Posts about <%= @city.name %></h3>
<br>
<br>
  
<% @city.posts.each do |post| %> 
  <div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-6">
      <%= link_to post.title, post_path(post) %>
      
    </div>
  </div>
<% end %>
<%= link_to "add new post", new_post_path %>