<h1>Profile Page</h1>
<div>
  <%= image_tag @city.avatar.url(:medium) %>
  <h1><%= @city.name %> </h1>
</div>
<h3>Posts about <%= @city.name %></h3>
<% @posts.each do |p| %> 
  <div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-6">
      <%= p.title %>
      <%= p.description %>
    </div>
  </div>
<% end %>


</div>